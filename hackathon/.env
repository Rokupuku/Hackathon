# 백엔드 서버 연결 설정
REACT_APP_API_BASE_URL=https://jimcarry.onrender.com
REACT_APP_API_VERSION=v1

# 환경 설정
REACT_APP_ENV=development
REACT_APP_DEBUG=true

# API 엔드포인트 설정
REACT_APP_API_ENDPOINTS_CHECKLIST=/api/checklist
REACT_APP_API_ENDPOINTS_SURVEY=/api/survey
REACT_APP_API_ENDPOINTS_AUTH=/api/auth

# 인증 설정
REACT_APP_JWT_SECRET=your_jwt_secret_here
REACT_APP_TOKEN_EXPIRY=3600

# 파일 업로드 설정
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif

# 로깅 설정
REACT_APP_LOG_LEVEL=info
REACT_APP_ENABLE_ANALYTICS=false